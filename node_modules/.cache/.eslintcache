id;\n        dispatch({\n            type: \"update_user\",\n            targetID: targetID\n        }) \n    }\n\n\n    return (\n        <>\n            <div className=\"welcome_bar\">\n            <h4 className=\"welcome_text\">\n                Cafe Kora Customers \n            </h4>\n            <p className=\"welcome_ptag\">Total Customers: {getTotalCustomers(users)}</p>\n            \n        </div>\n        {\n            users?.length ?\n\n        <>\n        <div className=\"operations_section\">\n\n        </div>\n        <div className=\"display_area\">\n            <table className=\"customers_table\">\n                <thead>\n                    <tr>\n                        <th>First Name</th>\n                        <th>Last Name</th>\n                        <th>Email Address</th>\n                        <th>Phone Number</th>\n                        <th>Date Joined</th>\n                        <th>Action</th>\n                    </tr>\n                </thead>\n                <tbody>\n                {\n                    users.map( customer => \n                        customer.type === \"customer\" &&\n                        <tr key={customer.userID} id={customer.userID}>\n                            <td>{customer.fname}</td>\n                            <td>{customer.lname}</td>\n                            <td>{customer.email}</td>\n                            <td>{customer.phone}</td>\n                            <td>{customer.datejoined.toDate().toDateString()}</td>\n                            <td><p><Delete onClick={handleDelete} className=\"action_icons\"/>\n                            <Link to={`/admin/update_products/${customer.userID}`}>\n                                <Create\n                                className=\"action_icons\"\n                                />                                \n                            </Link>\n                            </p>\n                            </td>\n                    