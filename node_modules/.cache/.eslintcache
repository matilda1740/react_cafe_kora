        if(window.confirm(\"Are You Sure you want to delete this record\")){\n            dispatch({\n                type: \"delete_user\",\n                targetID: targetID\n            }) \n        }\n    }\n\n    const handleUpdate = (e) => {\n        let targetID = e.target.parentNode.parentNode.parentNode.id;\n        dispatch({\n            type: \"update_user\",\n            targetID: targetID\n        }) \n    }\n\n    return (\n        <>\n        {\n            users?.length ?\n        <>\n        <div className=\"welcome_bar\">\n            <h4 className=\"welcome_text\">\n                Cafe Kora Team\n            </h4>\n            <p className=\"welcome_ptag\">Total Team Members: {getTotalTeam(users)}</p>\n            \n        </div>\n\n        <div className=\"display_area\">\n            <table className=\"customers_table\">\n                <thead>\n                    <tr>\n                        <th>First Name</th>\n                        <th>Last Name</th>\n                        <th>Email Address</th>\n                        <th>Phone Number</th>\n                        <th>Date Joined</th>\n                        <th>Action</th>\n                    </tr>\n                </thead>\n            <tbody>\n            {\n                users.map( team => \n                    team.type === \"admin\" &&\n                    <tr key={team.userID} id={team.userID}>\n                        <td>{team.fname}</td>\n                        <td>{team.lname}</td>\n                        <td>{team.email}</td>\n                        <td>{team.phone}</td>\n                        <td>{team.datejoined.toDate().toDateString()}</td>\n                        <td><p><Delete onClick={handleDelete} className=\"action_icons\"/>\n                        <Link to={`/admin/update_products/${team.userID}`}>\n                            <Create\n                            className=\"action_icons\"\n                            />                                \n                        </Link>\n                        </p>\n                        </td>\n                    </tr>\n                )\n        \n            }\n            </tbody>    \n\n            </table>\n        </div> \n        </>\n            :\n            <div className=\"welcome_bar\">\n            <p className=\"welcome_ptag\">Loading Employees...</p>\n            </div>     \n        }\n           \n\n        </>\n\n    )\n}\n","/Users/matildamwendwa/Documents/Strathmore/Year 2/Web Application Dev/react_semester_project/src/reducers/rootReducer.js",[],["208","209"],"/Users/matildamwendwa/Documents/Strathmore/Year 2/Web Application Dev/react_semester_project/src/reducers/productReducer.js",[],"/Users/matildamwendwa/Documents/Strathmore/Year 2/Web Application Dev/react_semester_project/src/reducers/userReducer.js",[],"/Users/matildamwendwa/Documents/Strathmore/Year 2/Web Application Dev/react_semester_project/src/actions/types.js",[],["210","211"],"/Users/matildamwendwa/Documents/Strathmore/Year 2/Web Application Dev/react_semester_project/src/components/adminComponents/ViewProducts.js",[],"/Users/matildamwendwa/Documents/Strathmore/Year 2/Web Application Dev/react_semester_project/src/components/Profile.js",["212","213","214","215"],"import React, { useState } from 'react'\nimport { useHistory } from 'react-router-dom';\nimport { useStateValue } from './StateProvider'\nimport '../components/adminComponents/Update.css'\nimport './Profile.css'\n\nexport default function Profile({match}) {\n\n    const [error, setError] = useState();\n    const [isError, setIsError] = useState(false);\n\n    const handleUpdate = () => {\n    }\n    const handleChange = input => async (e) => {\n    }\n\n    return (\n        <form onSubmit={handleUpdate}  className=\"update_right registration_form\">\n            <h2 className=\"add_prod_title\">Cafe Kora User Profile</h2>\n            {isError &&\n            <div className=\"form_error form_inputs\">\n         